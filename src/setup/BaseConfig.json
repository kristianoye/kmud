{
  "mud": {
    "features": {
      "autoWiz": false,
      "intermud3": true,
      "webEditor": true
    },
    "passwordPolicy": {
      "allowPlainTextAuth": true,
      "minLength": 3,
      "maxLength": 100,
      "requiredUpper": 1,
      "requiredLower": 1,
      "requiredSymbols": 1,
      "requiredNumbers": 1
    },
    "portBindings": []
  },
  "mudlib": {
    "baseDirectory": "./lib",
    "defaultError": "What? (not recognized: $verb)",
    "heartbeatInterval": "1 second",
    "objectResetInterval": "30 minutes",
    "includePath": [
      "/base/include",
      "/sys/include"
    ],
    "applyNames": {
      "getPreloads": "epilog",
      "validExec": "validExec",
      "validRead": "validRead",
      "validWrite": "validWrite"
    },
    "inGameMaster": {
      "path": "/sys/daemon/GameMaster",
      "parameters": {
        "permissionsFile": "/sys/etc/permissions.json",
        "preloadsFile": "/sys/etc/preloads.json",
        "groupsFile": "/sys/etc/groups.json",
        "accessFile": "/sys/etc/access.json"
      }
    },
    "logDirectory": "/log",
    "loginObject": "/sys/lib/Login",
    "simulEfuns": "/sys/lib/SimulEfuns"
  },
  "driver": {
    "core": "single-core",
    "features": {
      "creatorStats": true,
      "domainStats": false
    },
    "maxCommandLength": 10240,
    "resetPollingInterval": "5 seconds",
    "useLazyResets": true,
    "useObjectProxies": false,
    "useRevocableProxies": false,
    "objectCreationMethod": "fullWrapper",
    "compiler": {
      "maxCompileTime": "1.5 seconds",
      "maxConstructorTime": 1000,
      "virtualMachine": "vm",
      "components": [
        {
          "id": "JSXTranspiler",
          "name": "JSX Transpiler",
          "file": "./compiler/JSXTranspiler"
        },
        {
          "id": "LPCPreprocessor",
          "name": "MudOS Preprocessor",
          "file": "./compiler/LPCPreprocessor",
          "reusable": false
        }
      ],
      "languages": {
        ".js": {
          "id": "JavaScript",
          "loader": "MUDOSLoader",
          "name": "JavaScript Pipeline",
          "pipeline": []
        },
        ".jsx": {
          "id": "JSX",
          "enabled": true,
          "loader": "MUDOSLoader",
          "name": "MUD JSX",
          "pipeline": [
            "JSXTranspiler"
          ]
        },
        ".c": {
          "id": "LPC",
          "enabled": false,
          "loader": "MUDOSLoader",
          "name": "LPC Pipeline",
          "pipeline": [
            "LPCPreprocessor"
          ]
        }
      },
      "loaders": {
        "MUDLoader": {
          "name": "KMUD Standard Loader",
          "file": "./MUDLoader"
        },
        "MUDOSLoader": {
          "name": "MudOS Compatibility Loader",
          "file": "./MUDOSLoader"
        }
      }
    },
    "networking": {
      "endpoints": {
        "http": {
          "enabled": true,
          "handlers": [
            {
              "id": "default-http",
              "file": "./network/HTTPClientEndpoint",
              "name": "Standard Web Server (HTTP)",
              "default": true
            }
          ]
        },
        "https": {
          "enabled": false,
          "handlers": []
        },
        "telnet": {
          "enabled": true,
          "handlers": [
            {
              "id": "ranvier-telnet",
              "file": "./network/RanvierTelnetEndpoint",
              "name": "Ranvier Telnet Server",
              "default": true
            }
          ]
        }
      }
    }
  }
}